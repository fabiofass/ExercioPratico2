package aula05;

public class Arreys {

	public static void main(String[] args) {
		int[] vetor = new int [5];
		vetor[0] = 2;
		for(int i =0; 2*i<5;i++) {
		System.out.println(vetor[0]);
		System.out.println(vetor[1]);
		System.out.println(vetor[2]);
		System.out.println(vetor[3]);
		System.out.println(vetor[4]);

		}
	}

}
